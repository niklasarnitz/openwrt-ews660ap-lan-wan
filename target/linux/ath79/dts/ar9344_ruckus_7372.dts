/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

#include "ar9344.dtsi"

/ {
	compatible = "ruckus,7372", "qca,ar9344";
	model = "Ruckus ZoneFlex 7372";

	aliases {
		led-boot = &dir;
		led-failsafe = &dir;
		led-running = &dir;
		led-upgrade = &dir;
		label-mac-device = &eth0;
	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&gpio 0 GPIO_ACTIVE_HIGH>;
			debounce-interval = <60>;
		};
	};

	leds {
		compatible = "gpio-leds";

		pinctrl-names = "default";
		pinctrl-0 = <&jtag_disable_pins>;

		power_red {
			label = "7372:red:power";
			gpios = <&gpio 16 GPIO_ACTIVE_LOW>;
			default-state = "on";
		};

		lan {
			label = "7372:green:lan";
			gpios = <&gpio 13 GPIO_ACTIVE_LOW>;
		};

		dir: dir {
			label = "7372:green:dir";
			gpios = <&gpio 17 GPIO_ACTIVE_LOW>;
		};

		air {
			label = "7372:green:air";
			gpios = <&gpio 18 GPIO_ACTIVE_LOW>;
		};

		wmac {
			label = "7372:green:wmac";
			gpios = <&gpio 19 GPIO_ACTIVE_LOW>;
		};

		wmac_act {
			label = "7372:amber:wmac";
			gpios = <&gpio 20 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy0tpt";
		};

		ath9k {
			label = "7372:green:ath9k";
			gpios = <&gpio 21 GPIO_ACTIVE_LOW>;
		};

		ath9k_act {
			label = "7372:amber:ath9k";
			gpios = <&gpio 22 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy1tpt";
		};
	};
};

&ref {
	clock-frequency = <40000000>;
};

&uart {
	status = "okay";
};

&spi {
	status = "okay";

	num-cs = <1>;

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <40000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			uboot: partition@0 {
				label = "u-boot";
				reg = <0x000000 0x040000>;
				read-only;
			};

			partition@40000 {
				compatible = "denx,uimage";
				label = "firmware";
				reg = <0x040000 0xf00000>;
			};

			partition@f40000 {
				label = "u-boot-env";
				reg = <0xf40000 0x040000>;
			};

			art: partition@f80000 {
				label = "art";
				reg = <0xf80000 0x080000>;
				read-only;
			};

			partition@1000000 {
				label = "firmware2";
				reg = <0x1000000 0x1000000>;
			};
		};
	};
};

&mdio0 {
	status = "okay";

	phy6: ethernet-phy@6 {
		reg = <6>;
		eee-broken-100tx;
		eee-broken-1000t;
	};
};

&eth0 {
	status = "okay";

	mtd-mac-address = <&art 0x60>;

	phy-handle = <&phy6>;
	phy-mode = "rgmii-id";

	pll-data = <0x0e000000 0x08000101 0x08001313>;

	gmac-config {
		device = <&gmac>;
		rgmii-gmac0 = <1>;
		rxdv-delay = <3>;
		rxd-delay = <3>;
		txen-delay = <3>;
		txd-delay = <3>;
	};
};

&eth1 {
	status = "okay";

	mtd-mac-address = <&art 0x66>;

	phy-handle = <&swphy0>;
};

&wmac {
	status = "okay";

	mtd-cal-data = <&art 0x41000>;
	mtd-mac-address = <&art 0x6c>;
};

&pcie {
	status = "okay";

	wifi@168c,0033 {
		compatible = "pci168c,0033";
		reg = <0x0 0 0 0 0>;
		mtd-mac-address = <&art 0x76>;
	};
};
